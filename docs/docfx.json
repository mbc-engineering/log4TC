{
  "metadata": [
    {
      "src": [
        {
          "files": [ "**/*.csproj" ],
          "exclude": [ "**/bin/**", "**/obj/**" ],
          "src": "../source/Log4Tc"
        }
      ],
      "dest": "api",
      "properties": {
        "TargetFramework": "net461"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "**/**.md",
          "**/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "api/**",
          "Docker/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/images/**",
          "**/res/*.png",
          "**/_assets/*.png",
          "favicon.ico"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "Dokumentation mbc Log4Tc",
      "_appFooter": "<span>Copyright © 2020 <br><strong>mbc</strong>-engineering GmbH</span>",
      "_enableSearch": "true",
      "_appFaviconPath": "favicon.ico"
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "template/material"
    ],
    "postProcessors": [
      "ExtractSearchIndex"
    ],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "disableGitFeatures": true,
    "disableDefaultFilter": false
  }
}