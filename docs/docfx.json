{
  "metadata": [],
  "build": {
    "content": [
      {
        "files": [
          "**/**.md",
          "**/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "api/**",
          "Docker/**",
          "internal/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/images/**",
          "**/res/*.png",
          "**/assets/*.png",
          "**/assets/*.svg",
          "favicon.ico",
          "logo.svg"
        ],
        "exclude": [
          "internal/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**",
          "internal/**"
        ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "Dokumentation mbc Log4Tc",
      "_appFooter": "<div style=\"display: flex; align-items: center; gap: 20px;\"> <div id=\"google_translate_element\"></div> <script type=\"text/javascript\">  function googleTranslateElementInit() {    new google.translate.TranslateElement({      pageLanguage: 'de',       includedLanguages: 'en,de,fr,es',      layout: google.translate.TranslateElement.InlineLayout.SIMPLE    }, 'google_translate_element');  }<\/script><script type=\"text/javascript\"         src=\"https://translate.google.com/translate_a/element.js?hl=en&cb=googleTranslateElementInit\"><\/script>   <div><span>Help us on <a href=\"https://github.com/mbc-engineering/log4TC\"><strong>github</strong></a></span></div> </div>",
      "_enableSearch": "true",
      "_appFaviconPath": "favicon.ico",
      "pdf": true,
      "pdfTocPage": true,
      "pdfFileName": "log4TC.pdf",
      "pdfCoverPage": "index.html"
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "sitemap": {
      "baseUrl": "https://mbc-engineering.github.io/log4TC",
      "priority": 0.5,
      "changefreq": "daily"
    },
    "dest": "_site",
    "template": [
      "default",
      "modern",
      "template"
    ],
    "postProcessors": [
      "ExtractSearchIndex"
    ],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "disableGitFeatures": true,
    "disableDefaultFilter": false
  }
}
