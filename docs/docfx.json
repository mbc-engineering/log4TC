{
  "metadata": [],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "**/**.md",
          "**/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "api/**",
          "Docker/**",
          "internal/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/images/**",
          "**/res/*.png",
          "**/assets/*.png",
          "favicon.ico",
          "logo.svg"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "Dokumentation mbc Log4Tc",
      "_appFooter": "<span>Copyright © 2020 <br><strong>mbc</strong>-engineering GmbH</span>",
      "_enableSearch": "true",
      "_appFaviconPath": "favicon.ico"
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "template/material"
    ],
    "postProcessors": [
      "ExtractSearchIndex"
    ],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "disableGitFeatures": true,
    "disableDefaultFilter": false
  }
}