{
  "metadata": [],
  "build": {
    "content": [
      {
        "files": [
          "**/**.md",
          "**/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "api/**",
          "Docker/**",
          "internal/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/images/**",
          "**/res/*.png",
          "**/assets/*.png",
          "**/assets/*.svg",
          "favicon.ico",
          "logo.svg"
        ],
        "exclude": [
          "internal/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**",
          "internal/**"
        ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "Dokumentation mbc Log4Tc",
      "_appFooter": "<span>Help us on <a href=\"https://github.com/mbc-engineering/log4TC\"><br><strong>github</strong></a></span>",
      "_enableSearch": "true",
      "_appFaviconPath": "favicon.ico",
      "pdf": true,
      "pdfTocPage": true,
      "pdfFileName": "log4TC.pdf",
      "pdfCoverPage": "index.html"
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "sitemap": {
      "baseUrl": "https://mbc-engineering.github.io/log4TC",
      "priority": 0.5,
      "changefreq": "daily"
    },
    "dest": "_site",
    "template": [
      "default",
      "modern",
      "template"
    ],
    "postProcessors": [
      "ExtractSearchIndex"
    ],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "disableGitFeatures": true,
    "disableDefaultFilter": false
  }
}
