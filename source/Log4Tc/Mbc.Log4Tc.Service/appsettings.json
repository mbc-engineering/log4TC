{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Outputs": [
    {
      "Type": "nlog",
      "ExcludeFilter": {
        "Logger": "influxdb.*"
      }
    },
    {
      "Type": "influxdb",
      "Filter": { "Logger": "influxdb.*" },
      "Config": {
        "Url": "http://mbc-srv02:8086",
        "Database": "log4tc"
      }
    },
    {
      "Type": "graylog",
      "Filter": { "Level": "Info" },
      "Config": {
        "GraylogHostname": "mbc-srv02",
        "GraylogPort": 12201
      }
    }
  ]
}
